public class Question8 {

    public static void main(String[] args) {

        // Input array
        int[] arr = {1, 2, 3, 4, 5};

        // Initialize product variable
        int product = 1;

        // Iterate through the array and multiply each element with product
        for (int i = 0; i < arr.length; i++) {
            product *= arr[i];
        }

        // Print the product
        System.out.println("The product of all array elements is: " + product);
    }
}

